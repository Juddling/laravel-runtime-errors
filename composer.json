{
    "name": "juddling/laravel-runtime-errors",
    "description": "Catch errors before runtime in your Laravel project",
    "type": "library",
    "authors": [
        {
            "name": "James Judd",
            "email": "james.judd@netsells.co.uk"
        },
        {
            "name": "Karl Roberts",
            "email": "karlos545@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.0",
        "illuminate/console": "^5.0",
        "nikic/php-parser": "^3.1 | ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Juddling\\RouteChecker\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juddling\\RouteChecker\\RouteCheckerServiceProvider"
            ]
        }
    }
}
